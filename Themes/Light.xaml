<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="PrimaryForegroundBrush" Color="#121212"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#D0D0D0"/>
    <SolidColorBrush x:Key="AccentBrush" Color="#007ACC"/>
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#888888"/>
</ResourceDictionary>