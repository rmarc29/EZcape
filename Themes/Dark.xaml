<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBackgroundBrush" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="#2D2D30"/>
    <SolidColorBrush x:Key="PrimaryForegroundBrush" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#434346"/>
    <SolidColorBrush x:Key="AccentBrush" Color="#0097FF"/>
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#A0A0A0"/>
</ResourceDictionary>